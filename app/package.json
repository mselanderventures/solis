        "@types/react-router-dom": "^5.3.0",
        "@types/validator": "^13.6.3",
        "antd": "4.6.3",
        "apollo": "^2.33.6",
        "apollo-upload-client": "^16.0.0",
        "bootstrap": "^4.6.0",
        "concurrent": "^0.3.2",
        "cross-env": "5.0.5",
        "feather-icons": "^4.28.0",
        "feather-icons-react": "^0.4.3",
        "firebase": "^9.1.1",
        "graphql": "^15.6.0",
        "less": "^3.11.1",
        "next": "10.0.4",
        "rc-field-form": "^1.20.0",
        "react": "^16.13.0",
        "react-bootstrap": "^1.6.0",
        "react-dom": "^16.13.0",
