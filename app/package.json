{
    "name": "@SVM/app",
    "version": "1.0.0",
    "scripts": {
        "dev": "next dev",
        "build": "lessc -js ./src/styles/antd.less ./src/styles/antd.css && next build",
        "start": "next start",
        "download-schema": "apollo service:download --endpoint=http://localhost:8000/graphql/ ./src/apollo/graphql-schema.json",
        "codegen": "yarn download-schema && apollo codegen:generate --localSchemaFile=./src/apollo/graphql-schema.json --target=typescript --tagName=gql"
    },
    "main": "dist/index",
    "types": "dist/index",
    "license": "UNLICENSED",
